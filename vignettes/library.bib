keywords = {J-NNS,Nutrition surveys,Representativeness,Sample,Selection bias},
number = {9},
pages = {964--966},
pmid = {16039832},
title = {{Is the national nutrition survey in Japan representative of the entire Japanese population?}},
volume = {21},
year = {2005}
}
