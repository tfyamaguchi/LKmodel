doi = {10.1111/j.1600-0765.2008.01085.x},
file = {:C$\backslash$:/Users/301602/Documents/Mendeley Desktop/2008/Furugen et al. - 2008 - The relationship between periodontal condition and serum levels of resistin and adiponectin in elderly Japanese.pdf:pdf},
issn = {00223484},
journal = {Journal of Periodontal Research},
keywords = {Adiponectin,Elderly,Periodontitis,Resistin},
number = {5},
