doi = {10.1002/(SICI)1097-0010(20000515)80:7<1033::AID-JSFA595>3.0.CO;2-T},
isbn = {1097-0010},
issn = {0022-5142},
journal = {Journal of the Science of Food and Agriculture},
keywords = {absorption,acids,avenathramides,caffeic,caffeicins,caffeoylquinic acids,caffeoylsucci-,caftaric,chicoric,chlorogenic acids,chlorogenic lactones,cinnamates,coutaric,cynarin,dicaffeoylquinic acids,dietary burden,fertaric,ferulic,feruloylquinic acids,glutathionylcaftaric,metabolism,noylquinic acids,p -coumaric,p -coumaroylquinic acids,phaseolic,phenylindans,review,rosmarinic,sinapic},
