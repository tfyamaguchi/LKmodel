isbn = {1475-2662 (Electronic)$\backslash$r0007-1145 (Linking)},
issn = {14752662},
journal = {British Journal of Nutrition},
keywords = {Catechins,Postprandial antioxidant capacity,Postprandial glucose,Postprandial oxidative stress},
number = {9},
pages = {1542--1550},
