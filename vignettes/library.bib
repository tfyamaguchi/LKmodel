journal = {Preventive Medicine},
keywords = {Cholesterol,Exercise,Lipids,Lipoproteins,Meta-analysis,Physical fitness},
number = {5},
pages = {651--661},
pmid = {15066369},
title = {{Walking, lipids, and lipoproteins: A meta-analysis of randomized controlled trials}},
volume = {38},
