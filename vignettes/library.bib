journal = {Ecological Modelling},
keywords = {Decomposition,Ecosystem model,Energy,Fragility,Nutrient imbalance,Resilience,Synthesis},
pages = {12--18},
publisher = {Elsevier B.V.},
title = {{Productivity, nutrient imbalance and fragility in coupled producer-decomposer systems}},
url = {http://dx.doi.org/10.1016/j.ecolmodel.2012.05.020},
volume = {245},
year = {2012}
}
