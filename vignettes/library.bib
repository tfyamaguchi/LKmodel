keywords = {Bioimpedance,Electrical tissue modeling,Fat monitoring,Finite elements,Subcutaneous fat,Visceral fat},
month = {jun},
number = {6},
pages = {975--982},
pmid = {15977727},
title = {{Fat and Hydration Monitoring by Abdominal Bioimpedance Analysis: Data Interpretation by Hierarchical Electrical Modeling}},
