@article{Akitsuki2011,
abstract = {To investigate the neural underpinnings of the effect of nutrition, brain activity of six young healthy volunteers who had a breakfast including various nutrients was compared to when they skipped breakfast or had only sugar for breakfast by functional magnetic resonance imaging. A repeated measure counterbalanced crossover design was employed. We demonstrated that significantly higher brain activation was observed in the medial aspect of the prefrontal cortex when the subjects had a nutritionally balanced breakfast while the subjects were conducting N-back tasks. This preliminary report was the first to demonstrate by means of brain imaging techniques that taking various nutrients as breakfast as well sugar has relevant impacts on underlying physiological events or cognition.},
