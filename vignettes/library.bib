journal = {International Journal of Eating Disorders},
keywords = {Brain dopamine,Eating behaviors,Neurobiologic processes,Restraint},
number = {2},
pages = {136--142},
pmid = {12616579},
title = {{Brain dopamine is associated with eating behaviors in humans}},
volume = {33},
year = {2003}
}
