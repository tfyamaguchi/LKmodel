@article{Simmons2005,
abstract = {Increasing research indicates that concepts are represented as distributed circuits of property information across the brain's modality-specific areas. The current study examines the distributed representation of an important but under-explored category, foods. Participants viewed pictures of appetizing foods (along with pictures of locations for comparison) during event-related fMRI. Compared to location pictures, food pictures activated the right insula/operculum and the left orbitofrontal cortex, both gustatory processing areas. Food pictures also activated regions of visual cortex that represent object shape. Together these areas contribute to a distributed neural circuit that represents food knowledge. Not only does this circuit become active during the tasting of actual foods, it also becomes active while viewing food pictures. Via the process of pattern completion, food pictures activate gustatory regions of the circuit to produce conceptual inferences about taste. Consistent with theories that ground knowledge in the modalities, these inferences arise as reenactments of modality-specific processing.},
author = {Simmons, W. Kyle and Martin, Alex and Barsalou, Lawrence W.},
doi = {10.1093/cercor/bhi038},
isbn = {1047-3211 (Print)$\backslash$r1047-3211 (Linking)},
issn = {10473211},
journal = {Cerebral Cortex},
keywords = {Concepts,Insula/operculum,Knowledge,Orbitofrontal cortex,fMRI},
number = {10},
pages = {1602--1608},
pmid = {15703257},
title = {{Pictures of appetizing foods activate gustatory cortices for taste and reward}},
volume = {15},
year = {2005}
}
@misc{Turner2000,
abstract = {In this paper we explore the potential of multilevel models for meta-analysis$\backslash$nof trials with binary outcomes for both summary data, such as log-odds$\backslash$nratios, and individual patient data. Conventional fixed effect and$\backslash$nrandom effects models are put into a multilevel model framework,$\backslash$nwhich provides maximum likelihood or restricted maximum likelihood$\backslash$nestimation. To exemplify the methods, we use the results from 22$\backslash$ntrials to prevent respiratory tract infections; we also make comparisons$\backslash$nwith a second example data set comprising fewer trials. Within summary$\backslash$ndata methods, confidence intervals for the overall treatment effect$\backslash$nand for the between-trial variance may be derived from likelihood$\backslash$nbased methods or a parametric bootstrap as well as from Wald methods;$\backslash$nthe bootstrap intervals are preferred because they relax the assumptions$\backslash$nrequired by the other two methods. When modelling individual patient$\backslash$ndata, a bias corrected bootstrap may be used to provide unbiased$\backslash$nestimation and correctly located confidence intervals; this method$\backslash$nis particularly valuable for the between-trial variance. The trial$\backslash$neffects may be modelled as either fixed or random within individual$\backslash$ndata models, and we discuss the corresponding assumptions and implications.$\backslash$nIf random trial effects are used, the covariance between these and$\backslash$nthe random treatment effects should be included; the resulting model$\backslash$nis equivalent to a bivariate approach to meta-analysis. Having implemented$\backslash$nthese techniques, the flexibility of multilevel modelling may be$\backslash$nexploited in facilitating extensions to standard meta-analysis methods.},
author = {Turner, R M and Omar, R Z and Yang, M and Goldstein, H and Thompson, S G},
booktitle = {Stat Med},
doi = {10.1002/1097-0258(20001230)19:24<3417::AID-SIM614>3.0.CO;2-L [pii]},
isbn = {0277-6715 (Print)$\backslash$r0277-6715 (Linking)},
issn = {0277-6715},
keywords = {Clinical Trials as Topic,Clinical Trials as Topic: statistics {\&} numerical d,Confidence Intervals,Female,Humans,Likelihood Functions,Logistic Models,Meta-Analysis as Topic,Models,Odds Ratio,Outcome Assessment (Health Care),Pre-Eclampsia,Pre-Eclampsia: drug therapy,Pregnancy,Respiratory Tract Infections,Respiratory Tract Infections: drug therapy,Statistical},
number = {24},
pages = {3417--3432},
pmid = {11122505},
title = {{A multilevel model framework for meta-analysis of clinical trials with binary outcomes}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11122505},
volume = {19},
year = {2000}
}
@article{Jehl2015,
abstract = {Electrical Impedance Tomography (EIT) is a noninvasive imaging modality, where imperceptible currents are applied to the skin and the resulting surface voltages are measured. It has the potential to distinguish between ischaemic and haemorrhagic stroke with a portable and inexpensive device. The image reconstruction relies on an accurate forward model of the experimental setup. Because of the relatively small signal in stroke EIT, the finite element modelling requires meshes of more than 10 million elements. To study the requirements in the forward modelling in EIT and also to reduce the time for experimental image acquisition it is necessary to reduce the run time of the forward computation. We show the implementation of a parallel forward solver for EIT using the DUNE-FEM C++ library and demonstrate its performance on many CPU's of a computer cluster. For a typical EIT application a direct solver was significantly slower and not an alternative to iterative solvers with multigrid preconditioning. With this new solver we can compute the forward solutions and the Jacobian matrix of a typical EIT application with 30 electrodes on a 15 million element mesh in less than 15 minutes. This makes it a valuable tool for simulation studies and EIT applications with high precision requirements. It is freely available for download.},
author = {Jehl, Markus and Dedner, Andreas and Betcke, Timo and Aristovich, Kirill and Klofkorn, Robert and Holder, David},
doi = {10.1109/TBME.2014.2342280},
file = {:C$\backslash$:/Users/301602/Documents/Mendeley Desktop/2015/Jehl et al. - 2015 - A fast parallel solver for the forward problem in electrical impedance tomography.pdf:pdf},
issn = {15582531},
journal = {IEEE Transactions on Biomedical Engineering},
keywords = {Electrical impedance tomography (EIT),finiteelement solver,forward problem,parallel computing},
number = {1},
pages = {126--137},
pmid = {25069109},
title = {{A fast parallel solver for the forward problem in electrical impedance tomography}},
volume = {62},
year = {2015}
}
@misc{Best1995,
abstract = {Quantification of the average and interindividual variation in pharmacokinetic$\backslash$nbehavior within the patient population is an important aspect of$\backslash$ndrug development. Population pharmacokinetic models typically involve$\backslash$nlarge numbers of parameters related nonlinearly to sparse, observational$\backslash$ndata, which creates difficulties for conventional methods of analysis.$\backslash$nThe nonlinear mixed-effects method implemented in the computer program$\backslash$nNONMEM is a widely used approach to the estimation of population$\backslash$nparameters. However, the method relies on somewhat restrictive modeling$\backslash$nassumptions to enable efficient parameter estimation. In this paper$\backslash$nwe describe a Bayesian approach to population pharmacokinetic analysis$\backslash$nwhich used a technique known as Gibbs sampling to simulate values$\backslash$nfor each model parameter. We provide details of how to implement$\backslash$nthe method in the context of population pharmacokinetic analysis,$\backslash$nand illustrate this via an application to gentamicin population pharmacokinetics$\backslash$nin neonates.},
author = {Best, N G and Tan, K K and Gilks, W R and Spiegelhalter, D J},
booktitle = {J Pharmacokinet Biopharm},
issn = {0090-466X},
keywords = {Anti-Bacterial Agents,Anti-Bacterial Agents: pharmacokinetics,Bayes Theorem,Gentamicins,Gentamicins: pharmacokinetics,Humans,Infant,Models,Newborn,Statistical},
number = {4},
pages = {407--435},
pmid = {8882748},
title = {{Estimation of population pharmacokinetics using the Gibbs sampler}},
