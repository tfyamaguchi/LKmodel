isbn = {0002-9165 (Print)$\backslash$r0002-9165 (Linking)},
issn = {00029165},
journal = {American Journal of Clinical Nutrition},
keywords = {Blood lipids,Blood pressure,Cardiovascular disease risk,Diet,Homocysteine,Isoflavones,Oxidized LDL,Soy protein},
