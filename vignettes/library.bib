journal = {Obesity Research {\&} Clinical Practice},
number = {1},
pages = {e16--e25},
pmid = {24548573},
title = {{Association of dietary factors with abdominal subcutaneous and visceral adiposity in Japanese men}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1871403X12000415},
volume = {8},
year = {2014}
}
