@article{Havermans2010,
abstract = {Sensory-specific satiety (SSS) refers to the reduction in pleasantness of a food with its consumption relative to other unconsumed foods. In a typical SSS paradigm, the participants receive a range of food items to taste and evaluate and then one of the foods is consumed ad libitum until satiation. After the consumption of the test food, all the foods (including the test food) are then again tasted and evaluated. If SSS is the result of habituation this evaluation of the test food after its consumption would be subject to dishabituation (i.e. recovery of SSS) if the other unconsumed foods are evaluated first. To examine whether this is the case a total of 57 participants were randomly assigned to one of two groups: Test Food First (TFF; n=28) or Test Food Last (TFL; n=29). We hypothesized that group TFF would show stronger SSS than TFL. We found clear indication of SSS, but the degree of SSS did not differ between the two groups (F[1,55] {\textless}1). This finding suggests that SSS is unaffected by the sequence of tasting food items with its assessment. The potential implications for understanding SSS in terms of habituation are discussed. ?? 2010 Elsevier Ltd.},
author = {Havermans, Remco C. and Siep, Nicolette and Jansen, Anita},
doi = {10.1016/j.appet.2010.05.088},
file = {:C$\backslash$:/Users/301602/Documents/Mendeley Desktop/2010/Havermans, Siep, Jansen - 2010 - Sensory-specific satiety is impervious to the tasting of other foods with its assessment.pdf:pdf},
isbn = {1095-8304 (Electronic)$\backslash$r0195-6663 (Linking)},
issn = {01956663},
journal = {Appetite},
keywords = {Habituation,Neural adaptation,Sensory-specific satiety},
number = {2},
pages = {196--200},
pmid = {20566398},
publisher = {Elsevier Ltd},
title = {{Sensory-specific satiety is impervious to the tasting of other foods with its assessment}},
url = {http://dx.doi.org/10.1016/j.appet.2010.05.088},
volume = {55},
year = {2010}
}
