@article{Slattery1998,
abstract = {Colon cancer has been associated with several nutrients and foods.$\backslash$nThe authors used data from a population-based study conducted in$\backslash$nNorthern California, Utah, and Minnesota to examine associations$\backslash$nbetween dietary eating patterns and risk of developing colon cancer.$\backslash$nThrough factor analysis, detailed dietary intake data obtained from$\backslash$n1,993 cases (diagnosed in 1991-1994) and 2,410 controls were grouped$\backslash$ninto factors that were evaluated for relations with lifestyle characteristics$\backslash$nand colon cancer risk. Several dietary patterns emerged. The dietary$\backslash$npatterns with the most variation were labeled "Western," "prudent,"$\backslash$n"high fat/sugar dairy," "substituters," and "drinkers." The "Western"$\backslash$ndietary pattern was associated with a higher body mass index and$\backslash$na greater intake of total energy and dietary cholesterol. The "prudent"$\backslash$npattern was associated with higher levels of vigorous leisure time$\backslash$nphysical activity, smaller body size, and higher intakes of dietary$\backslash$nfiber and folate. Persons who had high scores on the "drinker" pattern$\backslash$nwere also more likely to smoke cigarettes. The "Western" dietary$\backslash$npattern was associated with an increased risk of colon cancer in$\backslash$nboth men and women. The association was strongest among people diagnosed$\backslash$nprior to age 67 years (for men, odds ratio (OR)=1.96, 95{\%} confidence$\backslash$ninterval (CI) 1.22-3.15; for women, OR=2.02, 95{\%} CI 1.21-3.36) and$\backslash$namong men with distal tumors (OR=2.25, 95{\%} CI 1.47-3.46). The "prudent"$\backslash$ndiet was protective, with the strongest associations being observed$\backslash$namong people diagnosed prior to age 67 years (men: OR=0.63, 95{\%} CI$\backslash$n0.43-0.92; women: OR=0.58, 95{\%} CI 0.38-0.87); associations with this$\backslash$ndietary pattern were also strong among persons with proximal tumors$\backslash$n(men: OR=0.55, 95{\%} CI 0.38-0.80; women: OR=0.64, 95{\%} CI 0.45-0.92).$\backslash$nAlthough "substituters" (people who substituted low fat dairy products$\backslash$nfor high fat dairy products, margarine for butter, poultry for red$\backslash$nmeat, and whole grains for refined grains) were at reduced risk of$\backslash$ncolon cancer, the reduction in risk was not statistically significant.$\backslash$nThese data support the hypothesis that overall dietary intake pattern$\backslash$nis associated with colon cancer, and that the dietary pattern associated$\backslash$nwith the greatest increase in risk is the one which typifies a Western-style$\backslash$ndiet.},
author = {Slattery, M L and Boucher, K M and Caan, B J and Potter, J D and Ma, K N},
doi = {10.1093/aje/148.1.4-a},
isbn = {0002-9262 (Print)$\backslash$r0002-9262},
issn = {0002-9262},
journal = {Am J Epidemiol},
number = {1},
pages = {4--16},
pmid = {9663397},
title = {{Eating patterns and risk of colon cancer}},
volume = {148},
year = {1998}
}
