booktitle = {Int J Clin Pharmacol Ther},
doi = {10.5414/CPP42015},
file = {:C$\backslash$:/Users/301602/Documents/Mendeley Desktop/2004/L{\"{o}}tsch, Kobal, Geisslinger - 2004 - Programming of a flexible computer simulation to visualize pharmacokinetic-pharmacodynamic models.pdf:pdf},
isbn = {0946-1965},
