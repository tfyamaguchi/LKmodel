@article{Staessen2004,
abstract = {ContextSelf-measurement of blood pressure is increasingly used in clinical$\backslash$npractice, but how it affects the treatment of hypertension requires further$\backslash$nstudy.ObjectiveTo compare use of blood pressure (BP) measurements taken in physicians'$\backslash$noffices and at home in the treatment of patients with hypertension.Design, Setting, and ParticipantsBlinded randomized controlled trial conducted from March 1997 to April$\backslash$n2002 at 56 primary care practices and 3 hospital-based outpatient clinics$\backslash$nin Belgium and 1 specialized hypertension clinic in Dublin, Ireland. Four$\backslash$nhundred participants with a diastolic BP (DBP) of 95 mm Hg or more as measured$\backslash$nat physicians' offices were enrolled and followed up for 1 year.InterventionsAntihypertensive drug treatment was adjusted in a stepwise fashion based$\backslash$non either the self-measured DBP at home (average of 6 measurements per day$\backslash$nduring 1 week; n = 203) or the average of 3 sitting DBP readings at the physician's$\backslash$noffice (n = 197). If the DBP guiding treatment was above ({\textgreater}89 mm Hg), at (80-89$\backslash$nmm Hg), or below ({\textless}80 mm Hg) target, a physician blinded to randomization$\backslash$nintensified antihypertensive treatment, left it unchanged, or reduced it,$\backslash$nrespectively.Mean Outcome MeasuresOffice and home BP levels, 24-hour ambulatory BP, intensity of drug$\backslash$ntreatment, electrocardiographic and echocardiographic left ventricular mass,$\backslash$nsymptoms reported by questionnaire, and costs of treatment.ResultsAt the end of the study (median follow-up, 350 days; interquartile range,$\backslash$n326-409 days), more home BP than office BP patients had stopped antihypertensive$\backslash$ndrug treatment (25.6{\%} vs 11.3{\%}; P{\textless}.001) with no$\backslash$nsignificant difference in the proportions of patients progressing to multiple-drug$\backslash$ntreatment (38.7{\%} vs 45.1{\%}; P = .14). The final office,$\backslash$nhome, and 24-hour ambulatory BP measurements were higher (P{\textless}.001) in the home BP group than in the office BP group. The mean$\backslash$nbaseline-adjusted systolic/diastolic differences between the home and office$\backslash$nBP groups averaged 6.8/3.5 mm Hg, 4.9/2.9 mm Hg, and 4.9/2.9 mm Hg, respectively.$\backslash$nLeft ventricular mass and reported symptoms were similar in the 2 groups.$\backslash$nCosts per 100 patients followed up for 1 month were only slightly lower in$\backslash$nthe home BP group (€3875 vs €3522 [{\$}4921 vs {\$}4473]; P = .04).ConclusionsAdjustment of antihypertensive treatment based on home BP instead of$\backslash$noffice BP led to less intensive drug treatment and marginally lower costs$\backslash$nbut also to less BP control, with no differences in general well-being or$\backslash$nleft ventricular mass. Self-measurement allowed identification of patients$\backslash$nwith white-coat hypertension. Our findings support a stepwise strategy for$\backslash$nthe evaluation of BP in which self-measurement and ambulatory monitoring are$\backslash$ncomplementary to conventional office measurement and highlight the need for$\backslash$nprospective outcome studies to establish the normal range of home-measured$\backslash$nBP.},
author = {Staessen, Jan A.},
doi = {10.1001/jama.291.8.955},
issn = {0098-7484},
journal = {Jama},
number = {8},
pages = {955},
title = {{Antihypertensive Treatment Based on Blood Pressure Measurement at Home or in the Physician's Office{\textless}SUBTITLE{\textgreater}A Randomized Controlled Trial{\textless}/SUBTITLE{\textgreater}}},
url = {http://jama.jamanetwork.com/article.aspx?doi=10.1001/jama.291.8.955},
volume = {291},
year = {2004}
}
@article{Wang_2013_23861681,
abstract = {The relationship between adipose and bone tissues is still being debated. The purpose of our study was to evaluate whether the distribution and volume of abdomen adipose tissue are correlated to trabecular bone mineral density in the lumbar spine. In this cross-sectional study, 320 Chinese women, being divided into two groups according to age ≥55 years and {\textless}55 years, were evaluated with quantitative computed tomography (QCT) of the spine to simultaneously evaluate the average trabecular BMD of L2-L4, VAT, and SAT. Possible covariates of height, weight, age, and comorbidities were considered. In the {\textless}55-year-old sample, multiple linear regression analyses indicated that VAT volume was negatively correlated to trabecular BMD (P value = 0.0003) and SAT volume had no correlation to trabecular BMD. In contrast, there was no significant correlation between VAT or SAT and BMD in the ≥55-year-old sample. Our results indicate that high VAT volume is associated with low BMD in Chinese women aged {\textless}55 years and SAT has no relation with BMD.},
author = {Wang, Ling and Wang, Wei and Xu, Li and Cheng, Xiaoguang and Ma, Yimin and Liu, Dan and Guo, Zhe and Su, Yongbin and Wang, Qianqian},
doi = {10.1155/2013/378632},
