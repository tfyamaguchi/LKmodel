file = {:C$\backslash$:/Users/301602/Documents/Mendeley Desktop/2002/Kawano et al. - 2002 - Melanin-concentrating hormone neuron system the Wide Web that controls the feeding.pdf:pdf},
isbn = {1447-6959 (Print)},
issn = {1447-6959},
journal = {Anatomical science international},
keywords = {Animals,Brain,Brain: cytology,Brain: metabolism,DNA-Binding Proteins,DNA-Binding Proteins: metabolism,Feeding Behavior,Feeding Behavior: physiology,Humans,Hypothalamic Hormones,Hypothalamic Hormones: metabolism,Leptin,Leptin: metabolism,Melanins,Melanins: metabolism,Neural Pathways,Neural Pathways: cytology,Neural Pathways: metabolism,Neurons,Neurons: cytology,Neurons: metabolism,Pituitary Hormones,Pituitary Hormones: metabolism,Receptors, Cell Surface,Receptors, Cell Surface: metabolism,Receptors, Leptin,Receptors, Pituitary Hormone,Receptors, Pituitary Hormone: metabolism,STAT3 Transcription Factor,Trans-Activators,Trans-Activators: metabolism},
