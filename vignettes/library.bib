keywords = {FMRI,Low-fat labeling,Obesity,Reward circuitry,Top-down processing},
number = {1},
pages = {65--72},
pmid = {21497628},
publisher = {Elsevier Ltd},
title = {{An fMRI study of obesity, food reward, and perceived caloric density. Does a low-fat label make food less appealing?}},
