journal = {Journal of Food Science},
keywords = {Dried bonito,Flavor,Palatability,Saltiness enhancement,Umami},
number = {6},
pages = {321--325},
pmid = {19241577},
title = {{Saltiness enhancement by the characteristic flavor of dried bonito stock}},
volume = {73},
year = {2008}
