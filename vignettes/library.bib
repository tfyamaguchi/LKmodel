pages = {206--210},
pmid = {10757681},
title = {{Characteristics of bone mineral density and soft tissue composition of obese Japanese women: Application of dual-energy X-ray absorptiometry}},
url = {http://link.springer.com/10.1007/s007740050086},
volume = {17},
year = {1999}
}
@article{EuropeanFoodSafetyAuthorityEFSAAbstract2015,
abstract = {Following a request from the European Commission, the Panel on Dietetic Products, Nutrition and Allergies derived Dietary Reference Values for vitamin A. The Panel considered that a concentration of 20 $\mu$g retinol/g liver can be used as a target for establishing the Average Requirement (AR) for vitamin A. In the absence of a better characterisation of the relationship between vitamin A intake and liver stores, a factorial approach was applied. This approach considered a total body/liver retinol store ratio of 1.25, a liver/body weight ratio of 2.4 {\%}, a fractional catabolic rate of body retinol of 0.7 {\%} per day, an efficiency of storage in the whole body for ingested retinol of 50 {\%} and reference weights for women and men in the EU of 58.5 and 68.1 kg, respectively. ARs of 570 $\mu$g retinol equivalent (RE)/day for men and 490 $\mu$g RE/day for women were derived. Assuming a coefficient of variation (CV) of 15 {\%}, Population Reference Intakes (PRIs) of 750 $\mu$g RE/day for men and 650 $\mu$g RE/day for women were set. For infants aged 7–11 months and children, the same equation as for adults was applied by using specific values for reference weight and liver/body weight ratio. For catabolic rate, the adult value corrected on the basis of a growth factor was used. ARs range from 190 $\mu$g RE/day in infants aged 7–11 months to 580 $\mu$g RE/day in boys aged 15–17 years. PRIs for infants and children were estimated using a CV of 15 {\%} and range from 250 to 750 $\mu$g RE/day. For pregnancy and lactation, additional vitamin A requirements related to the accumulation of retinol in fetal and maternal tissues and transfer of retinol into breast milk were considered and PRIs of 700 and 1 300 $\mu$g RE/day, respectively, were set.},
author = {(EFSA), European Food Safety Authority},
doi = {10.2903/j.efsa.2015.4254},
file = {:C$\backslash$:/Users/301602/Documents/Mendeley Desktop/2015/(EFSA) - 2015 - Scientific Opinion on Dietary Reference Values for iron.pdf:pdf},
issn = {18314732},
journal = {EFSA Journal},
month = {oct},
number = {10},
pages = {4254},
title = {{Scientific Opinion on Dietary Reference Values for iron}},
