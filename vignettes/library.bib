issn = {00275107},
journal = {Mutation Research - Fundamental and Molecular Mechanisms of Mutagenesis},
keywords = {Epichlorohydrin,Hemoglobin adduct,High frequency cell,Micronucleus,Occupational,Sister chromatid exchange},
number = {2},
pages = {217--226},
pmid = {9434878},
title = {{Monitoring of occupational exposure to epichlorohydrin by genetic effects and hemoglobin adducts}},
