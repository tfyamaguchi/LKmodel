@misc{Herrmann1989,
abstract = {Phenolic acid compounds seem to be universally distributed in plants.$\backslash$nThey have been the subject of a great number of chemical, biological,$\backslash$nagricultural, and medical studies. Hydroxycinnamic acid compounds$\backslash$noccur most frequently as simple esters with hydroxy carboxylic acids$\backslash$nor glucose, while the hydroxybenzoic acid compounds are present mainly$\backslash$nin the form of glucosides. Furthermore, phenolic acids may occur$\backslash$nin food plants as esters or glycosides conjugated with other natural$\backslash$ncompounds such as flavonoids, alcohols, hydroxyfatty acids, sterols,$\backslash$nand glucosides. Also, hydroxycinnamic acid amides appear to be common$\backslash$nconstituents. The occurrence of the different natural phenolic acid$\backslash$ncompounds in foods is reviewed, and data of the content in fruit,$\backslash$nvegetables, and spices are given. The distribution of the main phenolic$\backslash$nacid compounds in food plants as well as their changes during development$\backslash$nand maturation of fruits are considered. Furthermore, the hydroxycinnamic$\backslash$nacids bound to cell wall polymers, the phenolic acid compounds in$\backslash$ncoffee, cereals, oil seed, and tree nuts, and the analysis of phenolic$\backslash$nacid derivatives are reviewed.},
author = {Herrmann, Karl},
booktitle = {Critical Reviews in Food Science and Nutrition},
doi = {10.1080/10408398909527504},
