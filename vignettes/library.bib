@article{Ford2002,
abstract = {ContextThe Third Report of the National Cholesterol Education Program Expert$\backslash$nPanel on Detection, Evaluation, and Treatment of High Blood Cholesterol in$\backslash$nAdults (ATP III) highlights the importance of treating patients with the metabolic$\backslash$nsyndrome to prevent cardiovascular disease. Limited information is available$\backslash$nabout the prevalence of the metabolic syndrome in the United States, however.ObjectiveTo estimate the prevalence of the metabolic syndrome in the United States$\backslash$nas defined by the ATP III report.Design, Setting, and ParticipantsAnalysis of data on 8814 men and women aged 20 years or older from the$\backslash$nThird National Health and Nutrition Examination Survey (1988-1994), a cross-sectional$\backslash$nhealth survey of a nationally representative sample of the noninstitutionalized$\backslash$ncivilian US population.Main Outcome MeasuresPrevalence of the metabolic syndrome as defined by ATP III (≥3 of$\backslash$nthe following abnormalities): waist circumference greater than 102 cm in men$\backslash$nand 88 cm in women; serum triglycerides level of at least 150 mg/dL (1.69$\backslash$nmmol/L); high-density lipoprotein cholesterol level of less than 40 mg/dL$\backslash$n(1.04 mmol/L) in men and 50 mg/dL (1.29 mmol/L) in women; blood pressure of$\backslash$nat least 130/85 mm Hg; or serum glucose level of at least 110 mg/dL (6.1 mmol/L).ResultsThe unadjusted and age-adjusted prevalences of the metabolic syndrome$\backslash$nwere 21.8{\%} and 23.7{\%}, respectively. The prevalence increased from 6.7{\%} among$\backslash$nparticipants aged 20 through 29 years to 43.5{\%} and 42.0{\%} for participants$\backslash$naged 60 through 69 years and aged at least 70 years, respectively. Mexican$\backslash$nAmericans had the highest age-adjusted prevalence of the metabolic syndrome$\backslash$n(31.9{\%}). The age-adjusted prevalence was similar for men (24.0{\%}) and women$\backslash$n(23.4{\%}). However, among African Americans, women had about a 57{\%} higher prevalence$\backslash$nthan men did and among Mexican Americans, women had about a 26{\%} higher prevalence$\backslash$nthan men did. Using 2000 census data, about 47 million US residents have the$\backslash$nmetabolic syndrome.ConclusionsThese results from a representative sample of US adults show that the$\backslash$nmetabolic syndrome is highly prevalent. The large numbers of US residents$\backslash$nwith the metabolic syndrome may have important implications for the health$\backslash$ncare sector.},
author = {Ford, Earl S. and Giles, Wayne H. and Dietz, William H.},
doi = {10.1001/jama.287.3.356},
file = {:C$\backslash$:/Users/301602/Documents/Mendeley Desktop/2002/Ford, Giles, Dietz - 2002 - Prevalence of the Metabolic Syndrome Among US Adults.pdf:pdf},
isbn = {0098-7484 (Print)},
issn = {0098-7484},
journal = {Jama},
