journal = {International Journal of Obesity},
keywords = {Liver fat,Obesity,Tea catechins,Visceral fat,$\beta$-oxidation},
number = {11},
pages = {1459--1464},
pmid = {12439647},
title = {{Beneficial effects of tea catechins on diet-induced obesity: Stimulation of lipid catabolism in the liver}},
volume = {26},
year = {2002}
}
