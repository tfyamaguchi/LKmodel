@article{Yanai2008,
abstract = {OBJECTIVE: Diacylglycerol (DAG)-rich oil has been suggested to suppress postprandial hyperlipidemia and promote negative caloric balance by increasing energy expenditure (EE), due to small intestine physiochemical dynamics that differ from triacylglycerol (TAG). We studied the effect of DAG on postprandial glucose/insulin metabolism by loading of carbohydrate with oil. Further, to reveal the mechanism for increased EE by DAG, we measured plasma serotonin, which is mostly present in the small intestine and mediates peripheral sympathetic thermogenesis. METHODS AND PROCEDURES: Randomized crossover study with 2-week wash-out interval between differing fat ingestion. Seven male, lean, Japanese students ingested DAG or TAG oil with 40 g of carbohydrate. Measurements of metabolic parameters were performed before and at 2, 4, and 6 h after fat ingestion. Plasma serotonin levels and cholesterol concentration in each lipoprotein were measured using high-performance liquid chromatography (HPLC). RESULTS: The substitution of DAG for TAG decreased very-low-density lipoprotein-cholesterol (VLDL-C) by 45.6{\%} at 2 h, and decreased serum insulin by 41.3{\%} at 4 h after ingestion. The incremental area under the curve (IAUC) for VLDL-C was positively correlated with the IAUC for insulin. Concurrently, DAG elevated plasma serotonin levels by 47.3{\%} at 2 h, while TAG did not influence. DISCUSSION: This study indicates that the substitution of DAG for TAG suppresses the postprandial increase in serum VLDL-C and insulin. This study also demonstrates that DAG ingestion increases plasma serotonin, proposing a possible mechanism for a postprandial increase in EE by DAG.},
author = {Yanai, Hidekatsu and Yoshida, Hiroshi and Tomono, Yoshiharu and Hirowatari, Yuji and Kurosawa, Hideo and Matsumoto, Akiyo and Tada, Norio},
doi = {10.1038/oby.2007.46},
file = {:C$\backslash$:/Users/301602/Documents/Mendeley Desktop/2008/Yanai et al. - 2008 - Effects of diacylglycerol on glucose, lipid metabolism, and plasma serotonin levels in Lean Japanese.pdf:pdf},
isbn = {1930-7381 (Print)$\backslash$r1930-7381},
issn = {19307381},
journal = {Obesity},
number = {1},
pages = {47--51},
pmid = {18223611},
title = {{Effects of diacylglycerol on glucose, lipid metabolism, and plasma serotonin levels in Lean Japanese}},
volume = {16},
year = {2008}
}
@misc{Chan1996a,
abstract = {OBJECTIVE: To examine the pattern of the metabolic syndrome in Chinese$\backslash$nand the causal relationships among its components, including aging,$\backslash$nobesity, hyperglycemia, hypertension, hypertriglyceridemia, and hyperinsulinemia$\backslash$nin these subjects. RESEARCH DESIGN AND METHODS: Based on a 75-g oral$\backslash$nglucose tolerance test, the World Health Organization criteria were$\backslash$nused for the diagnosis of glucose intolerance in a population-based$\backslash$nstudy involving 1.513 Chinese subjects in two work sites. Demographic$\backslash$ndata including age, family history of diabetes, BMI, waist-to-hip$\backslash$nratio (WHR), and sitting blood pressure (BP) were documented. Fasting$\backslash$nplasma glucose, triglyceride (TG) and insulin concentrations, and$\backslash$nspot urinary albumin concentration (Ualb) were also measured. Structural$\backslash$nequation modeling incorporating factor analysis and path analysis$\backslash$nwas performed to examine the causal relationships among these variables$\backslash$nand their interactions. RESULTS: Subjects who were treated with antidiabetic$\backslash$nand/or antihypertensive drugs or who had a plasma creatinine level$\backslash$n{\textgreater} or = 150 mumol/l were excluded (n = 52). The prevalence of diabetes$\backslash$nand impaired glucose tolerance (IGT) were 3.9{\%} (n = 34) and 7.2{\%}$\backslash$n(n = 63) in men (n = 881) and 3.1{\%} (n = 18) and 6.7{\%} (n = 39) in$\backslash$nwomen (n = 580), respectively. In both groups, glucose intolerance$\backslash$nwas associated with increasing age, higher BMI, WHR, BP, Ualb, serum$\backslash$nTG, and insulin levels as well as higher prevalence rates of positive$\backslash$nfamily history of diabetes. Structural equation modeling showed that$\backslash$nage was a significant determinant for both BMI and WHR. Age and obesity$\backslash$naccounted for most of the variance of BP, Ualb, plasma glucose, insulin$\backslash$nand TG levels either directly or indirectly. Plasma glucose was determined$\backslash$nby a positive family history of diabetes, age, and BMI while TG was$\backslash$ndependent on BMI and WHR. Serum insulin was mainly determined by$\backslash$na positive family history of diabetes, obesity, plasma glucose, and$\backslash$nTG levels. Apart from age and obesity, BP was also determined by$\backslash$nserum insulin, both of which had causal effects on Ualb. CONCLUSIONS:$\backslash$nThis model emphasizes the centrality of aging and obesity as well$\backslash$nas a positive family history of diabetes as major determinants of$\backslash$nthe components of the metabolic syndrome. These components in turn$\backslash$nhad causal effects upon one another. Apart from a familial tendency,$\backslash$na central neurohormonal mechanism may account for these abnormalities$\backslash$nmediated primarily through obesity and in close association with$\backslash$naging.},
author = {Chan, Juliana C.N. and Cheung, Jacky C.K. and Lau, Edith M.C. and Woo{\`{a}}, Jean and Chan, Albert Y.W. and Swaminathan, Ramasmyiyer and Cockrama, Clive S.},
booktitle = {Diabetes Care},
