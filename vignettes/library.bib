journal = {Brain},
keywords = {abbreviations,anova ϭ analysis of,ba ϭ brodmann area,feeding,motivation,neuroimaging,ofc ϭ orbitofrontal cortex,orbitofrontal cortex,rcbf ϭ regional,taste,variance},
number = {9},
pages = {1720--1733},
pmid = {11522575},
title = {{Changes in brain activity related to eating chocolate}},
url = {http://brain.oxfordjournals.org/content/124/9/1720{\%}5Cnhttp://brain.oxfordjournals.org/content/124/9/1720.short{\%}5Cnhttp://brain.oxfordjournals.org/content/brain/124/9/1720.full.pdf{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/11522575},
