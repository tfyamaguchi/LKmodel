issn = {09394753},
journal = {Nutrition, Metabolism and Cardiovascular Diseases},
keywords = {Chlorogenic acid,Coffee,Hydroxyhydroquinone,Hypertension,Randomized controlled trial},
number = {6},
pages = {408--414},
pmid = {17951035},
title = {{Hydroxyhydroquinone-free coffee: A double-blind, randomized controlled dose-response study of blood pressure}},
