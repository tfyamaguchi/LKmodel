keywords = {closed data,fuzzy sets,geostatistics,indicator kriging,ternary diagram},
number = {8},
pages = {951--966},
title = {{Compositional Kriging : A Spatial Interpolation Method for Compositional Data}},
url = {http://link.springer.com/article/10.1023/A:1012250107121},
volume = {33},
year = {2001}
