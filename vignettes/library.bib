journal = {Physiology and Behavior},
keywords = {Amnesic,Food intake,Memory recall,Recent eating},
number = {5},
pages = {734--739},
pmid = {18486159},
title = {{Cognitive influences on food intake: The effects of manipulating memory for recent eating}},
volume = {94},
year = {2008}
}
