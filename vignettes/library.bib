issn = {0918-2918},
journal = {Internal Medicine},
keywords = {10,2014,2169,299-305,53,9476,doi,dyslipidemia,glucose tolerance,intern med 53,internalmedicine,metabolic syndrome,visceral fat},
number = {4},
pages = {299--305},
pmid = {24531085},
title = {{Fat Accumulation and Obesity-related Cardiovascular Risk Factors in Middle-aged Japanese Men and Women}},
url = {http://jlc.jst.go.jp/DN/JST.JSTAGE/internalmedicine/53.9476?lang=en{\&}from=CrossRef{\&}type=abstract},
