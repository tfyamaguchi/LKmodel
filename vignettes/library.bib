file = {:C$\backslash$:/Users/301602/Documents/Mendeley Desktop/2011/Fiebig - 2011 - Determination of ester-bound 3-chloro-1,2-propanediol and glycidol in fats and oils - a collaborative study.pdf:pdf},
issn = {14387697},
journal = {European Journal of Lipid Science and Technology},
keywords = {3-MCPD-esters,Collaborative trial,GC-MS,Glycidyl esters,ISO 5725},
number = {3},
pages = {393--399},
title = {{Determination of ester-bound 3-chloro-1,2-propanediol and glycidol in fats and oils - a collaborative study}},
