file = {:C$\backslash$:/Users/301602/Documents/Mendeley Desktop/2005/Harada et al. - 2005 - Effects of the Long-Term Ingestion of Tea Catechins on Energy Expenditure and Dietary Fat Oxidation in Healthy Su.pdf:pdf},
issn = {1344-9702},
journal = {Journal of Health Science},
number = {2},
pages = {248--252},
title = {{Effects of the Long-Term Ingestion of Tea Catechins on Energy Expenditure and Dietary Fat Oxidation in Healthy Subjects}},
url = {http://joi.jlc.jst.go.jp/JST.JSTAGE/jhs/51.248?from=Google{\%}5Cnhttp://joi.jlc.jst.go.jp/JST.JSTAGE/jhs/51.248?from=CrossRef},
