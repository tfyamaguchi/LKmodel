@article{Jafar2003,
abstract = {BACKGROUND: Angiotensin-converting enzyme (ACE) inhibitors reduce$\backslash$nblood pressure and urine protein excretion and slow the progression$\backslash$nof chronic kidney disease. PURPOSE: To determine the levels of blood$\backslash$npressure and urine protein excretion associated with the lowest risk$\backslash$nfor progression of chronic kidney disease during antihypertensive$\backslash$ntherapy with and without ACE inhibitors. DATA SOURCES: 11 randomized,$\backslash$ncontrolled trials comparing the efficacy of antihypertensive regimens$\backslash$nwith or without ACE inhibitors for patients with predominantly nondiabetic$\backslash$nkidney disease. STUDY SELECTION: MEDLINE database search for English-language$\backslash$nstudies published between 1977 and 1999. DATA EXTRACTION: Data on$\backslash$n1860 nondiabetic patients were pooled in a patient-level meta-analysis.$\backslash$nProgression of kidney disease was defined as a doubling of baseline$\backslash$nserum creatinine level or onset of kidney failure. Multivariable$\backslash$nregression analysis was performed to assess the association of systolic$\backslash$nand diastolic blood pressure and urine protein excretion with kidney$\backslash$ndisease progression at 22 610 patient visits. DATA SYNTHESIS: Mean$\backslash$nduration of follow-up was 2.2 years. Kidney disease progression was$\backslash$ndocumented in 311 patients. Systolic blood pressure of 110 to 129$\backslash$nmm Hg and urine protein excretion less than 2.0 g/d were associated$\backslash$nwith the lowest risk for kidney disease progression. Angiotensin-converting$\backslash$nenzyme inhibitors remained beneficial after adjustment for blood$\backslash$npressure and urine protein excretion (relative risk, 0.67 [95{\%} CI,$\backslash$n0.53 to 0.84]). The increased risk for kidney progression at higher$\backslash$nsystolic blood pressure levels was greater in patients with urine$\backslash$nprotein excretion greater than 1.0 g/d (P {\textless} 0.006). CONCLUSION: Although$\backslash$nreverse causation cannot be excluded with certainty, a systolic blood$\backslash$npressure goal between 110 and 129 mm Hg may be beneficial in patients$\backslash$nwith urine protein excretion greater than 1.0 g/d. Systolic blood$\backslash$npressure less than 110 mm Hg may be associated with a higher risk$\backslash$nfor kidney disease progression.},
author = {Jafar, T H and Stark, P C and Schmid, C H and Landa, M and Maschio, G and de Jong, P E and de Zeeuw, D and Shahinfar, S and Toto, R and Levey, a S},
