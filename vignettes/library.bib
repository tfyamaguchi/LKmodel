journal = {Dyna},
keywords = {ambiental,cokriging,como,compositional kriging,de extracci{\'{o}}n exclusivamente a,de la distribuci{\'{o}}n del,de vista econ{\'{o}}mico,del yacimiento,fundamental,fuzzy kriging,granite,granito por calidades en,las zonas m{\'{a}}s rentables,para determinar la rentabilidad,para dirigir las labores,quality estimation,realizar una estimaci{\'{o}}n precisa,reduciendo as{\'{i}} la,resumen,tanto desde el punto,un yacimiento es},
