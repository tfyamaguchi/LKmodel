file = {:C$\backslash$:/Users/301602/Documents/Mendeley Desktop/2014/Martinez et al. - 2014 - MRI-compatible device for examining brain activation related to stepping.pdf:pdf},
isbn = {0278-0062 VO  - 33},
issn = {1558254X},
journal = {IEEE Transactions on Medical Imaging},
keywords = {Blood oxygen level dependent (BOLD) analysis,brain,dimensionality reduction,functional magnetic resonance imaging (fMRI),lower limbs movements,magnetic resonance imaging (MRI) compatible device,neural network},
number = {5},
pages = {1044--1053},
pmid = {24770910},
title = {{MRI-compatible device for examining brain activation related to stepping}},
volume = {33},
year = {2014}
}
