@misc{2009a,
abstract = {本論文は腹部の三次元形状から内臓脂肪面積を推定する方法について述べている.内臓脂肪面積はメタボリックシンドロームの診断にとって重要な指標であるが,一般的にはそれを腹囲長から推定している.本研究ではレンジファインダによって腹部形状を測定し,臍断面の輪郭線の形状を解析する.滑らかで大局的な形状を定量的に表現するためにフーリエ記述子を用いる.フーリエ記述子の構成する空間において主成分分析を行い,形状の特徴を抽出する.有効な主成分を利用して重回帰分析を行い,内臓脂肪面積との相関を最適化する推定式を導出する.実験では肥満患者21名の腹部形状データとX線CTを利用して測定した内臓脂肪面積,皮下脂肪面積を対象とした.この結果,腹囲長と内臓脂肪面積との相関は0.57しかないが,提案手法による内臓脂肪面積の推定値と実際の値との相関は0.82と高いことが確認できた.},
author = {齋藤, 仙一 and 亀嶋, 英人 and 佐藤, 幸男},
booktitle = {電子情報通信学会論文誌. D, 情報・システム},
file = {:C$\backslash$:/Users/301602/Documents/Mendeley Desktop/2009/齋藤, 亀嶋, 佐藤 - 2009 - 腹部断面形状からの内臓脂肪面積の推定法(生体工学).pdf:pdf},
issn = {18804535},
keywords = {フーリエ記述子,メタボリックシンドローム,三次元腹部形状,内臓脂肪面積推定,形状解析},
number = {11},
pages = {2059--2066},
title = {腹部断面形状からの内臓脂肪面積の推定法(生体工学)},
url = {http://ci.nii.ac.jp/naid/110007467246/},
volume = {92},
