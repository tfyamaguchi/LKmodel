@article{Ueno2012,
abstract = {A capacitive sensing method was developed for sensing surface electromyographic potential (sEMG) through a thin cloth. The method is based on capacitive coupling involving fabric electrodes, the cloth, and the skin of the subject. A proposed measuring device was assembled by modifying the previously developed instrument for electrocardiographic potential. Experimental results showed that sEMG obtained with the proposed system was clearly visible and showed firings synchronized with sEMG simultaneously measured with a commercial device and that the spectral powers of both sEMGs were almost identical. These results demonstrated the potential of the proposed method to measure sEMG through cloth. The method enables the discomfort due to conventional skin-to-electrode coupling to be eliminated, although the signal quality, such as the signal-to-noise ratio (S/N), is slightly decreased with the cloth barrier.},
author = {Ueno, Akinori and Yamaguchi, Toshihiko and Iida, Toshiya and Fukuoka, Yutaka and Uchikawa, Yoshinori and Noshiro, Makoto},
issn = {09144935},
journal = {Sensors and Materials},
keywords = {EMG measurement,capacitive electrode,drive-ground-plane technique,guarding technique,human-machine interfaces},
number = {6},
pages = {335--346},
title = {{Feasibility of Capacitive Sensing of Surface Electromyographic Potential through Cloth}},
url = {http://www.researchgate.net/publication/230788828{\_}Feasibility{\_}of{\_}Capacitive{\_}Sensing{\_}of{\_}Surface{\_}Electromyographic{\_}Potential{\_}through{\_}Cloth/file/79e415046cf783a161.pdf},
