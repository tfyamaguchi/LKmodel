@article{Flint2000,
abstract = {OBJECTIVE: To examine reproducibility and validity of visual analogue scales (VAS) for measurement of appetite sensations, with and without a diet standardization prior to the test days. DESIGN: On two different test days the subjects recorded their appetite sensations before breakfast and every 30 min during the 4.5 h postprandial period under exactly the same conditions. SUBJECTS: 55 healthy men (age 25.6AE AE0.6 y, BMI 22.6AE AE0.3 kga am 2). MEASUREMENTS: VAS were used to record hunger, satiety, fullness, prospective food consumption, desire to eat something fatty, salty, sweet or savoury, and palatability of the meals. Subsequently an ad libitum lunch was served and energy intake was recorded. Reproducibility was assessed by the coef{\textregistered}cient of repeatability (CR) of fasting, mean 4.5 h and peakanadir values. RESULTS: CRs (range 20 ± 61 mm) were larger for fasting and peaka anadir values compared with mean 4.5 h values. No parameter seemed to be improved by diet standardization. Using a paired design and a study power of 0.8, a difference of 10 mm on fasting and 5 mm on mean 4.5 h ratings can be detected with 18 subjects. When using desires to eat speci{\textregistered}c types of food or an unpaired design, more subjects are needed due to considerable variation. The best correlations of validity were found between 4.5 h mean VAS of the appetite parameters and subsequent energy intake (r  AE AE0.50 ± 0.53,.001). CONCLUSION: VAS scores are reliable for appetite research and do not seem to be in¯uenced by prior diet standardization. However, consideration should be given to the speci{\textregistered}c parameters being measured, their sensitivity and study power.},
archivePrefix = {arXiv},
arxivId = {NIHMS150003},
author = {Flint, A. and Raben, A. and Blundell, J. E. and Astrup, A.},
doi = {10.1038/sj.ijo.0801083},
eprint = {NIHMS150003},
file = {:C$\backslash$:/Users/301602/Documents/Mendeley Desktop/2000/Flint et al. - 2000 - Reproducibility, power and validity of visual analogue scales in assessment of appetite sensations in single test.pdf:pdf},
isbn = {0307-0565 (Print)$\backslash$r0307-0565 (Linking)},
issn = {03070565},
