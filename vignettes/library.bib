keywords = {Baseline risk,Control rate,Ecological bias,Heterogeneity,Hierarchical models,Multilevel models},
number = {7},
pages = {683--697},
pmid = {15358396},
title = {{Meta-regression detected associations between heterogeneous treatment effects and study-level, but not patient-level, factors}},
volume = {57},
year = {2004}
}
