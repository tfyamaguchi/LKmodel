issn = {0002-9165},
journal = {Am J Clin Nutr},
keywords = {Adult,Age Factors,Aged,Body Mass Index,Cholesterol, LDL/blood,Data Interpretation, Statistical,Diet,Dietary Fats/blood,Energy Intake/physiology,Female,Humans,Lipids/ blood,Longitudinal Studies,Male,Middle Aged,Models, Statistical,Pregnancy,Reproducibility of Results,Sex Factors},
