@article{Yamada2014,
abstract = {Bioelectrical impedance analysis (BIA) has been used to estimate skeletal muscle mass, but its application in the elderly is not optimal. The accuracy of BIA may be influenced by the expansion of extracellular water (ECW) relative to muscle mass with aging. Multifrequency BIA (MFBIA) can evaluate the distribution between ECW and intracellular water (ICW), and thus may be superior to single-frequency BIA (SFBIA) to estimate muscle mass in the elderly. A total of 58 elderly participants aged 65-85 years were recruited. Muscle cross-sectional area (CSA) was obtained from computed tomography scans at the mid-thigh. Segmental SFBIA and MFBIA were measured for the upper legs. An index of the ratio of ECW and ICW was calculated using MFBIA. The correlation between muscle CSA and SFBIA was moderate (r = 0.68), but strong between muscle CSA and MFBIA (r = 0.85). ECW/ICW index was significantly and positively correlated with age (P {\textless} 0.001). SFBIA tends to significantly overestimate muscle CSA in subjects who had relative expansion of ECW in the thigh segment (P {\textless} 0.001). This trend was not observed for MFBIA (P = 0.42). Relative expansion of ECW was observed in older participants. The relative expansion of ECW affects the validity of traditional SFBIA, which is lowered when estimating muscle CSA in the elderly. By contrast, MFBIA was not affected by water distribution in thigh segments, thus rendering the validity of MFBIA for estimating thigh muscle CSA higher than SFBIA in the elderly.},
author = {Yamada, Yosuke and Ikenaga, Masahiro and Takeda, Noriko and Morimura, Kazuhiro and Miyoshi, Nobuyuki and Kiyonaga, Akira and Kimura, Misaka and Higaki, Yasuki and Tanaka, Hiroaki},
doi = {10.1152/japplphysiol.00772.2013},
file = {:C$\backslash$:/Users/301602/Documents/Mendeley Desktop/2014/Yamada et al. - 2014 - Estimation of thigh muscle cross-sectional area by single- and multifrequency segmental bioelectrical impedance a.pdf:pdf},
isbn = {8175212543},
issn = {1522-1601},
journal = {Journal of applied physiology (Bethesda, Md. : 1985)},
keywords = {80 and over,Age Factors,Aged,Body Water,Body Water: physiology,Electric Impedance,Female,Humans,Leg,Leg: anatomy {\&} histology,Leg: physiology,Male,Muscle,Skeletal,Skeletal: anatomy {\&} histology,Skeletal: physiology,Thigh,Thigh: anatomy {\&} histology,Thigh: physiology},
number = {2},
