@article{Jenkins2002,
abstract = {BACKGROUND: Many of the benefits of soy have been attributed to soy isoflavones.$\backslash$n$\backslash$nOBJECTIVE: The objective was to determine the effects of high- and low-isoflavone soy-protein foods on both lipid and nonlipid risk factors for coronary artery disease (CAD).$\backslash$n$\backslash$nMETHODS: Forty-one hyperlipidemic men and postmenopausal women participated in a study with three 1-mo diets: a low-fat dairy food control diet and high- (50 g soy protein and 73 mg isoflavones daily) and low- (52 g soy protein and 10 mg isoflavones daily) isoflavone soyfood diets. All 3 diets were very low in saturated fat ({\textless} 5{\%} of energy) and cholesterol ({\textless} 50 mg/d). Fasting blood samples were drawn and blood pressure was measured at the start and end of each diet.$\backslash$n$\backslash$nRESULTS: No significant differences were seen between the high- and low-isoflavone soy diets. Compared with the control diet, however, both soy diets resulted in significantly lower total cholesterol, estimated CAD risk, and ratios of total to HDL cholesterol, LDL to HDL cholesterol, and apolipoprotein B to A-I. No significant sex differences were observed, except for systolic blood pressure, which in men was significantly lower after the soy diets than after the control diet. On the basis of blood lipid and blood pressure changes, the calculated CAD risk was significantly lower with the soy diets, by 10.1 +/- 2.7{\%}.$\backslash$n$\backslash$nCONCLUSION: Substitution of soyfoods for animal products, regardless of isoflavone concentration, reduces the CAD risk because of both modest reductions in blood lipids and reductions in oxidized LDL, homocysteine, and blood pressure.},
author = {Jenkins, David J.A. and Kendall, Cyril W.C. and Jackson, Chung Ja C. and Connelly, Philip W. and Parker, Tina and Faulkner, Dorothea and Vidgen, Edward and Cunnane, Stephen C. and Leiter, Lawrence A. and Josse, Robert G.},
