journal = {Biochemical and Biophysical Research Communications},
keywords = {Adipocytokines,Adiponectin,Adipose tissue,Bone mass,Bone metabolism,Osteoblasts,Osteoclasts,Osteopenia},
number = {2},
pages = {520--526},
pmid = {15850790},
title = {{Adiponectin increases bone mass by suppressing osteoclast and activating osteoblast}},
