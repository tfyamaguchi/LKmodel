isbn = {1053-8119 (Print)},
issn = {10538119},
journal = {Neuroimage},
number = {1},
pages = {43--53},
pmid = {9343556},
title = {{Anatomical mapping of functional activation in stereotactic coordinate space}},
volume = {1},
year = {1992}
}
