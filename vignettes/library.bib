pages = {115--26},
pmid = {10219951},
title = {{Computer-assisted diagnosis of breast cancer using a data-driven Bayesian belief network.}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1386505698001749 http://www.ncbi.nlm.nih.gov/pubmed/10219951},
