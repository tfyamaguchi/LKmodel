isbn = {1930-7381 (Print)$\backslash$r1930-7381 (Linking)},
issn = {1930-7381},
journal = {Obesity (Silver Spring, Md.)},
keywords = {adiposity,blood pressure,metabolic syndrome,serum cholesterol,tea polyphenol},
month = {jun},
number = {6},
pages = {1473--83},
pmid = {17557985},
title = {{A green tea extract high in catechins reduces body fat and cardiovascular risks in humans.}},
